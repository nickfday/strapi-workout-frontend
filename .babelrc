{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "src": "./src"
        },
        "extensions": [".ts", ".tsx", "js"]
      }
    ],
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ]
  ],
  "presets": ["next/babel"]
}
